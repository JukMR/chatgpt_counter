{

  "description": "Add a counter to the chat GPT chat window",
  "manifest_version": 2,
  "name": "chatgpt-counter",
  "version": "1.0",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.chat.openai.com/*"],
